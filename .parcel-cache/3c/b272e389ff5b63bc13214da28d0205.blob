*{box-sizing: border-box;}
*::before,*::after{box-sizing: border-box;}

.skin{
    position: relative;
}
.san{
    border: 10px solid red;
    border-color: red transparent transparent;
    border-bottom: none;
    width: 0px;
    height: 0px;
    position: relative;
    left: 50%;
    top: 200px;
    margin-left: -5px;
}

.yuan{
    position: absolute;
    width: 20px;
    height: 6px;
    background: red;
    top: -16px;
    left: -10px;
    border-radius: 10px 10px 0 0;

}

.eye{
    border: 2px solid #000;
    position: absolute;
    width: 64px;
    height: 64px;
    left: 50%;
    top: 100px;
    margin-left: -27px;
    background: #2e2e2e;
    border-radius: 50%;
}

.eye::before{
    content: '';
    display: block;
    border: 3px solid 000;
    width: 25px;
    height: 25px;
    background: #fff;
    border-radius: 50%;
    position: relative;
    left: 8px;
    top: 4px;

}

.eye.left{
transform: translateX(-100px);
}

.eye.right{
    transform: translateX(100px);
}
